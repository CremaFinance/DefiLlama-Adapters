{
  "name": "marinade-web",
  "version": "0.1.0",
  "private": true,
  "author": "marinade",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export && build-storybook -o ./out/sb",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "format": "prettier --write src",
    "up-latest": "yarn upgrade-interactive --latest",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "watch": "yarn run jest --watch",
    "docs": "typedoc"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@chakra-ui/layout": "^1.7.6",
    "@chakra-ui/react": "^1.8.6",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.8.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/lexend": "^4.5.5",
    "@fontsource/maven-pro": "^4.5.4",
    "@project-serum/anchor": "^0.20.1",
    "@quarryprotocol/quarry-sdk": "^3.0.0",
    "@saberhq/anchor-contrib": "^1.12.43",
    "@saberhq/solana-contrib": "^1.12.52",
    "@saberhq/stableswap-sdk": "^1.12.48",
    "@saberhq/token-utils": "^1.12.40",
    "@solana/spl-token": "^0.1.8",
    "@solana/spl-token-registry": "^0.2.2911",
    "@solana/wallet-adapter-base": "^0.9.4",
    "@solana/wallet-adapter-react": "^0.15.3",
    "@solana/wallet-adapter-wallets": "^0.15.5",
    "@solana/web3.js": "^1.35.1",
    "bignumber.js": "^9.0.2",
    "bn.js": "^5.2.0",
    "borsh": "^0.7.0",
    "bs58": "^5.0.0",
    "chart.js": "^3.7.1",
    "dayjs": "^1.10.8",
    "framer-motion": "^6.2.8",
    "graphql": "^16.3.0",
    "graphql-request": "^4.1.0",
    "html-react-parser": "^1.4.8",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3",
    "js-sha256": "^0.9.0",
    "jsbi": "^4.1.0",
    "mustache": "^4.2.0",
    "next": "^12.1.0",
    "next-export-i18n": "^1.4.3",
    "next-pwa": "^5.4.6",
    "next-seo": "^5.1.0",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.1",
    "react-dom": "^17.0.2",
    "react-gtm-module": "^2.0.11",
    "react-icons": "^4.3.1",
    "react-number-format": "^4.9.1",
    "react-query": "^3.34.16",
    "ts-jest": "^27.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.2.1",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@testing-library/jest-dom": "^5.16.2",
    "@types/bs58": "^4.0.1",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.40",
    "@types/react-gtm-module": "^2.0.1",
    "@types/uuid": "^8.3.4",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "commitlint": "^16.2.1",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-sznm": "^0.2.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-testing-library": "5.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "next-sitemap": "^2.5.9",
    "next-transpile-modules": "^9.0.0",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "typedoc": "^0.22.13",
    "typedoc-plugin-markdown": "^3.11.14",
    "typescript": "^4.6.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier --write"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "[[ -n $HUSKY_BYPASS ]] || commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
